/dts-v1/;

/ {
	description = "Buildroot kernel for Chromebook Kukui";
	#address-cells = <1>;

	images {
		kernel-1 {
			data = /incbin/("Image");
			type = "kernel_noload";
			arch = "arm64";
			os = "linux";
			compression = "none";
			load = <0>;
			entry = <0>;
		};
		fdt-1 {
			description = "kukui.dtb";
			data = /incbin/("mt8183-kukui-krane-sku176.dtb");
			type = "flat_dt";
			arch = "arm64";
			compression = "none";

			hash-1 {
				algo = "sha1";
			};
		};
	};

	configurations {
		default = "conf-1";
		conf-1 {
			kernel = "kernel-1";
			fdt = "fdt-1";
		};
	};
};
